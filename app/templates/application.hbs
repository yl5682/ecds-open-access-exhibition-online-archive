  <div class="pricing-header px-3 py-3 pt-md-5 pb-md-4 mx-auto text-center">
    <img src="assets/images/cover/cover.png" height="150px">
  </div>

  <div class="container">
    <div class="card-deck mb-3 text-center">
      <div class="card mb-4 box-shadow">
        <div class="card-header">
          <h4 class="my-0 font-weight-normal">Openness</h4>
        </div>
        <div class="card-body">
          <h1 class="card-title pricing-card-title"><img src="assets/images/icons/unlocked.svg" height="100px"></h1>
          <ul class="list-unstyled mt-3 mb-4">
            <li>Widening Potential Readership</li>
            <li>Reducing Market Barriers</li>
            <li>Engaging the Humanities</li>
            <li> &zwnj; </li>
          </ul>
          <button type="button" class="btn btn-lg btn-block btn-outline-primary">
            {{#photo-swipe items=orange history=false as |photoswipe|}}
              {{#each orange as |item index|}}
                <a {{action photoswipe.actions.open (hash index=index)}} >View</a>
              {{/each}}
            {{/photo-swipe}}
          </button>
        </div>
      </div>
      <div class="card mb-4 box-shadow">
        <div class="card-header">
          <h4 class="my-0 font-weight-normal">Accessibility</h4>
        </div>
        <div class="card-body">
          <h1 class="card-title pricing-card-title"><img src="assets/images/icons/key.svg" height="100px"></h1>
          <ul class="list-unstyled mt-3 mb-4">
            <li>Reducing Barriers to Access</li>
            <li>Sharing and Using</li>
            <li>Sustain the Sustained Argument</li>
            <li> &zwnj; </li>
          </ul>
          <button type="button" class="btn btn-lg btn-block btn-outline-primary">
            {{#photo-swipe items=items history=false as |photoswipe|}}
              {{#each items as |item index|}}
                <a {{action photoswipe.actions.open (hash index=index)}} >View</a>
              {{/each}}
            {{/photo-swipe}}
          </button>
        </div>
      </div>
      <div class="card mb-4 box-shadow">
        <div class="card-header">
          <h4 class="my-0 font-weight-normal">Possibilities</h4>
        </div>
        <div class="card-body">
          <h1 class="card-title pricing-card-title"><img src="assets/images/icons/fireworks.svg" height="100px"></h1>
          <ul class="list-unstyled mt-3 mb-4">
            <li>Immediate and Free</li>
            <li>New Possibilities</li>
            <li> &zwnj; </li>
            <li> &zwnj; </li>
          </ul>
          <button type="button" class="btn btn-lg btn-block btn-outline-primary">
            {{#photo-swipe items=black history=false as |photoswipe|}}
              {{#each black as |item index|}}
                <a {{action photoswipe.actions.open (hash index=index)}} >View</a>
              {{/each}}
            {{/photo-swipe}}
          </button>
        </div>
      </div>


    </div>

    <footer class="pt-4 my-md-5 pt-md-5">
      <div class="row">
        <div class="col-12 col-md">
